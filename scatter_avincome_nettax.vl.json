{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 400,
  "height": 300,
  "data": {
    "url": "https://raw.githubusercontent.com/shaheerrizwan786/3179/refs/heads/main/ts22datacleaned.csv",
    "format": {"type": "csv"}
  },
  "transform": [
    {"filter": "stateFilter == 'All' || datum.State == stateFilter"}
  ],
  "params": [
    {
      "name": "stateFilter",
      "value": "All",
      "bind": {
        "input": "select",
        "options": ["All", "ACT", "NSW", "NT", "QLD", "SA", "TAS", "VIC", "WA"],
        "name": "Select State: "
      }
    }
  ],
  "mark": {"type": "point", "size": 80, "opacity": 0.8},
  "encoding": {
    "x": {
      "field": "Average taxable income or loss",
      "type": "quantitative",
      "axis": {
        "title": "Average Taxable Income (AUD)",
        "grid": true,
        "labelFontSize": 12,
        "titleFontSize": 14
      }
    },
    "y": {
      "field": "Average net tax",
      "type": "quantitative",
      "axis": {
        "title": "Net Tax Paid (Average) (AUD)",
        "grid": true,
        "labelFontSize": 12,
        "titleFontSize": 14
      }
    },
    "color": {
      "field": "State",
      "type": "nominal",
      "legend": {"title": "State"}
    },
    "tooltip": [
      {"field": "Postcode", "title": "Postcode"},
      {"field": "State", "title": "State"},
      {
        "field": "Average taxable income or loss",
        "title": "Average Taxable Income",
        "format": "$,.2f"
      },
      {
        "field": "Average net tax",
        "title": "Net Tax Paid (Average)",
        "format": "$,.2f"
      }
    ]
  },
  "title": {
    "text": "Suburbs by Average Taxable Income within Each State",
    "fontSize": 15,
    "anchor": "start"
  },
  "config": {}
}